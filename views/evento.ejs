	<!DOCTYPE html>
<html lang="en">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta name="description" content="">

	<title>Habit Matcher</title>
	
	<!-- favicon -->
    <link rel="shortcut icon" href="images/ico/favicon.jpg">
    <link rel="GG" sizes="144x144" href="images/ico/GG.png">
    <link rel="G" sizes="114x114" href="images/ico/G.png">
    <link rel="M" sizes="72x72" href="images/ico/M.png">
    <link rel="P" href="images/ico/P.png">
	
	<!-- css -->
	<link rel="stylesheet" type="text/css" href="/css/bootstrap.min.css" />
	<link rel="stylesheet" type="text/css" href="/css/materialdesignicons.min.css" />
	<link rel="stylesheet" type="text/css" href="/css/jquery.mCustomScrollbar.min.css" />
	<link rel="stylesheet" type="text/css" href="/css/prettyPhoto.css" />
	<link rel="stylesheet" type="text/css" href="/css/unslider.css" />
	<link rel="stylesheet" type="text/css" href="/css/template.css" />

  <script>
    </script>

</head>


<body data-target="#nino-navbar" data-spy="scroll">

	<!-- Header
    ================================================== -->
	<header id="nino-header">
		<div id="nino-headerInner">					
			<nav id="nino-navbar" class="navbar navbar-default" role="navigation">
				<div class="container">

					<!-- Brand and toggle get grouped for better mobile display -->
					<div class="navbar-header">
						<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#nino-navbar-collapse">
							<span class="sr-only">Toggle navigation</span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
						</button>
						<a class="navbar-brand" href="perfil">Habit Matcher</a>
					</div>

					<!-- Collect the nav links, forms, and other content for toggling -->
					<div class="nino-menuItem pull-right">
						<div class="collapse navbar-collapse pull-left" id="nino-navbar-collapse">
							<ul class="nav navbar-nav">
								<li><a id="home" href="perfil">Meu Perfil</a></li>
								<script type="text/javascript">
									document.getElementById("home").onclick = function () {
    								location.href = "home";
								};</script>
								<li class="active"><a href="#seus_eventos">Meus Eventos</a></li>
								<li><a id="usuarios" href="usuarios">Encontre Pessoas</a></li>
								<script type="text/javascript">
								document.getElementById("usuarios").onclick = function () {
    								location.href = "usuarios";
								};</script>
								<script type="text/javascript">
								document.getElementById("usuarios").onclick = function () {
    								location.href = "usuarios";
								};</script>
								<li><a id="eventos" href="eventos">Encontre Eventos<span class="sr-only">(current)</span></a></li>
								<script type="text/javascript">
								document.getElementById("eventos").onclick = function () {
    								location.href = "eventos";
								};</script>
							</ul>
						</div><!-- /.navbar-collapse -->
						<ul class="nino-iconsGroup nav navbar-nav">
							<li><a id="login" href="#" class="nino-search"><i class="mdi mdi-logout nino-icon"></i></a></li>
							<script type="text/javascript">
								document.getElementById("login").onclick = function () {
    								location.href = "login";
								};</script>
						</ul>
					</div>
				</div><!-- /.container-fluid -->
			</nav>
		</div>
	</header><!--/#header-->

	<!-- Eventos
    ================================================== -->    

     <section id="seus_eventos">
		<div class="container">
			<h2 class="nino-sectionHeading">
				<span class="nino-subHeading">Evento</span>
				<%=evento[0].nome%>
			</h2>

			<center>
			<h3 class="articleTitle"><a href=""><%=evento[0].endereco%>, <%=evento[0].cidade%>, <%=evento[0].estado%></a></h3>
			<h5 class="articleTitle"><a href=""><%=evento[0].data%></a></h5>
			</center>
			<br>

			<div class="sectionContent">
				<div class="row nino-hoverEffect">
					<div class="item">
						<div class="overlay" href="#">
							
							<img width="600px" height="400px" alt="User Pic" src="https://safe-earth-53433.herokuapp.com/<%=evento[0].foto%>">
							
						</div>
					</div>
				</div>
			</div>
		</div>	
	</section>

<h2 class="nino-sectionHeading">Atividades</h2>
    <section id="interesses">
    	<div class="container">
    		<div layout="row" class="verticalStretch">
    			<%
    			t_ativ = lista_atividade.length;
    			for(var i=0; i < t_ativ; i++) { %>
    			<div class="item">
    				<div class="text"><%= lista_atividade[i] %></div>
    			</div>
    			<% } %>

    			<%for(var j=5-t_ativ; j>0; j--) { %>
    			<div class="item">
    			</div>
    			<% } %>
    		</div>
		</div>
    </section>
    <br>

	<h2 class="nino-sectionHeading">Alimentação</h2>
    <section id="interesses">
    	<div class="container">
    		<div layout="row" class="verticalStretch">
    			<%
    			t_alim = lista_alimentar.length;
    			for(var i=0; i < t_alim; i++) { %>
    			<div class="item">
    				<div class="text"><%= lista_alimentar[i] %></div>
    			</div>
    			<% } %>

    			<%for(var j=5-t_alim; j>0; j--) { %>
    			<div class="item">
    			</div>
    			<% } %>
    		</div>
		</div>
    </section><!--/#perfil-->
    <br>
    <br>

       <!-- Map
    ================================================== -->
    <section id="nino-map">
    	<div class="container">
    		<h2 class="nino-sectionHeading">
    			<i class="mdi mdi-map-marker nino-icon"></i>
    			<span class="text">Ver Rota</span>
    		</h2>
    		<br>
    		<div ><center>
    			<iframe
  				width="1200"
  				height="450"
  				frameborder="0" style="border:0"
  				src="https://www.google.com/maps/embed/v1/directions?key=AIzaSyDOx1AA-PnUiKf-9YGL8Ozx1hshfwnTNlk&origin=<%=end_usuario%>&destination=<%=end_evento%>" allowfullscreen>
				</iframe>
				</center>
	    	</div>
    	</div>
    </section><!--/#nino-map--> 

    <!-- Footer
    ================================================== -->
    <footer id="footer">
        <div class="container">
        	<div class="row">
        		<div class="col-md-4">
        			<div class="colInfo">

	        			
        			</div>
        		</div>
   
        	</div>
			<div class="nino-copyright">Copyright &copy; 2017</div>
        </div>
    </footer><!--/#footer-->

	<a href="#" id="nino-scrollToTop">Go to Top</a>
	
	<!-- javascript -->
	<script type="text/javascript" src="js/jquery.min.js"></script>	
	<script type="text/javascript" src="js/isotope.pkgd.min.js"></script>
	<script type="text/javascript" src="js/jquery.prettyPhoto.js"></script>
	<script type="text/javascript" src="js/bootstrap.min.js"></script>
	<script type="text/javascript" src="js/jquery.hoverdir.js"></script>
	<script type="text/javascript" src="js/modernizr.custom.97074.js"></script>
	<script type="text/javascript" src="js/jquery.mCustomScrollbar.concat.min.js"></script>
	<script type="text/javascript" src="js/unslider-min.js"></script>
	<script type="text/javascript" src="js/template.js"></script>

